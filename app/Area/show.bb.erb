<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @area.nombre %></h1>
    <%= link_to "Edit", :action => :edit, :id => @area.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Nombre</td>
        <td class="itemValue"><%= @area.nombre%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">CompromisoReduccion</td>
        <td class="itemValue"><%= @area.compromiso_reduccion%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">CompromisoIniciativas</td>
        <td class="itemValue"><%= @area.compromiso_iniciativas%></td>
      </tr>
      
    </table>
  </div>

</div>
