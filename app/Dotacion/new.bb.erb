<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New Dotacion</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @dotacion.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Contrato: </td>
                <td class="itemValue"><input type="text" name="dotacion[contrato_id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Fecha: </td>
                <td class="itemValue"><input type="text" name="dotacion[fecha]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Empleados: </td>
                <td class="itemValue"><input type="text" name="dotacion[empleados]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Recategorizacion: </td>
                <td class="itemValue"><input type="text" name="dotacion[recategorizacion]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Gestion dotacional: </td>
                <td class="itemValue"><input type="text" name="dotacion[gestion_dotacional]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Nuevos ingresos egresos: </td>
                <td class="itemValue"><input type="text" name="dotacion[nuevos_ingresos_egresos]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Created at: </td>
                <td class="itemValue"><input type="text" name="dotacion[created_at]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Modified at: </td>
                <td class="itemValue"><input type="text" name="dotacion[modified_at]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
