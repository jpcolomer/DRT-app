<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New Iniciativa</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @iniciativa.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Contrato: </td>
                <td class="itemValue"><input type="text" name="iniciativa[contrato_id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Compromiso: </td>
                <td class="itemValue"><input type="text" name="iniciativa[compromiso]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Created at: </td>
                <td class="itemValue"><input type="text" name="iniciativa[created_at]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Modified at: </td>
                <td class="itemValue"><input type="text" name="iniciativa[modified_at]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
