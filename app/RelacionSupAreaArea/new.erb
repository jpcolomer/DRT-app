<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @relacionsupareaarea.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @relacionsupareaarea.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="relacionsupareaarea[area_id]" class="fieldLabel">Area</label>
            <input type="text" id="relacionsupareaarea[area_id]" name="relacionsupareaarea[area_id]" <%= placeholder("Area") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="relacionsupareaarea[sup_area_id]" class="fieldLabel">Sup area</label>
            <input type="text" id="relacionsupareaarea[sup_area_id]" name="relacionsupareaarea[sup_area_id]" <%= placeholder("Sup area") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
