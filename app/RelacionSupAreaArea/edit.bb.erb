<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @relacionsupareaarea.object %>
    <h1>Edit <%= @relacionsupareaarea.area_id%></h1>
    <%= link_to "Delete", :action => :delete, :id => @relacionsupareaarea.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @relacionsupareaarea.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Area: </td>
              <td class="itemValue"><input type="text" name="relacionsupareaarea[area_id]" value="<%= @relacionsupareaarea.area_id %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Sup area: </td>
              <td class="itemValue"><input type="text" name="relacionsupareaarea[sup_area_id]" value="<%= @relacionsupareaarea.sup_area_id %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
