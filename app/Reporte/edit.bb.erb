<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @reporte.object %>
    <h1>Edit <%= @reporte.nombre%></h1>
    <%= link_to "Delete", :action => :delete, :id => @reporte.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @reporte.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Nombre: </td>
              <td class="itemValue"><input type="text" name="reporte[nombre]" value="<%= @reporte.nombre %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Url: </td>
              <td class="itemValue"><input type="text" name="reporte[url]" value="<%= @reporte.url %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
