<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New Reporte</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @reporte.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">File name: </td>
                <td class="itemValue"><input type="text" name="reporte[file_name]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Tipo: </td>
                <td class="itemValue"><input type="text" name="reporte[tipo]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Fecha: </td>
                <td class="itemValue"><input type="text" name="reporte[fecha]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Fecha actualizacion: </td>
                <td class="itemValue"><input type="text" name="reporte[fecha_actualizacion]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Url: </td>
                <td class="itemValue"><input type="text" name="reporte[url]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Url device: </td>
                <td class="itemValue"><input type="text" name="reporte[url_device]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
