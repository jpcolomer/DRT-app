<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @reporte.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @reporte.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="reporte[nombre]" class="fieldLabel">Nombre</label>
            <input type="text" id="reporte[nombre]" name="reporte[nombre]" <%= placeholder("Nombre") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="reporte[url]" class="fieldLabel">Url</label>
            <input type="text" id="reporte[url]" name="reporte[url]" <%= placeholder("Url") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
