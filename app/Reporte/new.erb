<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @reporte.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @reporte.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="reporte[file_name]" class="fieldLabel">File name</label>
            <input type="text" id="reporte[file_name]" name="reporte[file_name]" <%= placeholder("File name") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="reporte[tipo]" class="fieldLabel">Tipo</label>
            <input type="text" id="reporte[tipo]" name="reporte[tipo]" <%= placeholder("Tipo") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="reporte[fecha]" class="fieldLabel">Fecha</label>
            <input type="text" id="reporte[fecha]" name="reporte[fecha]" <%= placeholder("Fecha") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="reporte[fecha_actualizacion]" class="fieldLabel">Fecha actualizacion</label>
            <input type="text" id="reporte[fecha_actualizacion]" name="reporte[fecha_actualizacion]" <%= placeholder("Fecha actualizacion") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="reporte[url]" class="fieldLabel">Url</label>
            <input type="text" id="reporte[url]" name="reporte[url]" <%= placeholder("Url") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="reporte[url_device]" class="fieldLabel">Url device</label>
            <input type="text" id="reporte[url_device]" name="reporte[url_device]" <%= placeholder("Url device") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
