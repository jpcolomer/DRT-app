<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @avance_iniciativa.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @avance_iniciativa.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="avance_iniciativa[iniciativa_id]" class="fieldLabel">Iniciativa</label>
            <input type="text" id="avance_iniciativa[iniciativa_id]" name="avance_iniciativa[iniciativa_id]" <%= placeholder("Iniciativa") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="avance_iniciativa[valor]" class="fieldLabel">Avance</label>
            <input type="text" id="avance_iniciativa[valor]" name="avance_iniciativa[valor]" <%= placeholder("Valor") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="avance_iniciativa[fecha]" class="fieldLabel">Created at</label>
            <input type="text" id="avance_iniciativa[fecha]" name="avance_iniciativa[fecha]" <%= placeholder("Fecha") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
