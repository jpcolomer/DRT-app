<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New AvanceIniciativa</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @avance_iniciativa.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Iniciativa: </td>
                <td class="itemValue"><input type="text" name="avance_iniciativa[iniciativa_id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Avance: </td>
                <td class="itemValue"><input type="text" name="avance_iniciativa[avance]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Created at: </td>
                <td class="itemValue"><input type="text" name="avance_iniciativa[created_at]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Modified at: </td>
                <td class="itemValue"><input type="text" name="avance_iniciativa[modified_at]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
