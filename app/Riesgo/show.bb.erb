<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @riesgo.nemo %></h1>
    <%= link_to "Edit", :action => :edit, :id => @riesgo.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Nemo</td>
        <td class="itemValue"><%= @riesgo.nemo%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Probabilidad</td>
        <td class="itemValue"><%= @riesgo.probabilidad%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Impacto</td>
        <td class="itemValue"><%= @riesgo.impacto%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Descripcion</td>
        <td class="itemValue"><%= @riesgo.descripcion%></td>
      </tr>
      
    </table>
  </div>

</div>
